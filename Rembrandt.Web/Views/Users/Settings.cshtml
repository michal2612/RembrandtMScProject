@model Rembrandt.Contracts.Classes.Dataset.ContributorDto
@{
    ViewData["Title"] = "Contributor details";
}
<div class="form-group contributor-form">
    <label for="exampleFormControlSelect2">Example multiple select</label>
    <select class="form-control" id="exampleFormControlSelect2">
        <option value="0">Select your age range</option>
        <option>Below 29 years old</option>
        <option>Between 30 and 39 years old</option>
        <option>Between 40 and 49 years old</option>
        <option>Between 50 and 59 years old</option>
        <option>Above 60 years old</option>
    </select>

    <label for="exampleFormControlSelect2">Example multiple select</label>
    <select class="form-control" id="exampleFormControlSelect2">
        <option value="0">Select your gender</option>
        <option>Male</option>
        <option>Female</option>
    </select>

    <label for="exampleFormControlSelect2">Example multiple select</label>
    <select class="form-control" id="exampleFormControlSelect2">
        <option value="0">Select education level</option>
        <option>Primary School</option>
        <option>High School</option>
        <option>Vocational Training</option>
        <option>College Or University</option>
    </select>

    <label for="exampleFormControlSelect2">Visit frequency</label>
    <select class="form-control" id="exampleFormControlSelect2">
        <option value="0">Select an option</option>
        <option>Every day</option>
        <option>Several Times Per Week</option>
        <option>Once a week</option>
        <option>From 1 to 3 Times A Month</option>
        <option>Less Than Once A Month</option>
        <option>First Time Here</option>
        <option>I do not want to share this information.</option>
    </select>

    <label for="exampleFormControlSelect2">Visit frequency other parks</label>
    <select class="form-control" id="exampleFormControlSelect2">
        <option value="0">Select an option</option>
        <option>Every day</option>
        <option>Several Times Per Week</option>
        <option>Once a week</option>
        <option>From 1 to 3 Times A Month</option>
        <option>Less Than Once A Month</option>
        <option>First Time Here</option>
        <option>I do not want to share this information.</option>
    </select>

    <label for="exampleFormControlSelect2">Visit frequency other parks</label>
    <select class="form-control" id="exampleFormControlSelect2">
        <option value="0">Select an option</option>
        <option>Disagree</option>
        <option>Somewhat</option>
        <option>Agree</option>
        <option>I do not want to share this information.</option>
    </select>
    <hr style="width:50%;text-align:left;margin-left:0">
    <table class="table table-striped table-hover">
    <tr>
        <th>Please fill information about you:</th>
        <th></th>
        <th></th>
        <th></th>
    </tr>
    <tr>
        <td>
            Are you dutch nationality?
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="dutchNationality">
                <label class="form-check-label" for="inlineRadio1">Dutch</label>
            </div>
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="dutchNationality">
                <label class="form-check-label" for="inlineRadio2">Non-Dutch</label>
            </div>
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="dutchNationality">
                <label class="form-check-label" for="inlineRadio2">I do not want to share this information.</label>
            </div>
        </td>
    </tr>
    <tr>
        <td>
            Do you visit park daily?
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="VisitDaily">
                <label class="form-check-label" for="inlineRadio1">Visit Daily</label>
            </div>
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="VisitDaily">
                <label class="form-check-label" for="inlineRadio2">Not Visit Daily</label>
            </div>
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="VisitDaily">
                <label class="form-check-label" for="inlineRadio1">No opinion</label>
            </div>
        </td>
    </tr>
    <tr>
        <td>
            Do you visit alone?
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="VisitAlone">
                <label class="form-check-label" for="inlineRadio1">Visit alone</label>
            </div>
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="VisitAlone">
                <label class="form-check-label" for="inlineRadio2">Not alone</label>
            </div>
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="VisitAlone">
                <label class="form-check-label" for="inlineRadio1">No opinion</label>
            </div>
        </td>
    </tr>
    <tr>
        <td>
            Do you want to be more involved?
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="MoreInvolved">
                <label class="form-check-label" for="inlineRadio1">More involved</label>
            </div>          
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="MoreInvolved">
                <label class="form-check-label" for="inlineRadio2">I don't want to be more involved.</label>
            </div>    
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="MoreInvolved">
                <label class="form-check-label" for="inlineRadio1">No opinion</label>
            </div>
        </td>
    </tr>
    <tr>
        <td>
            Do you visit with children?
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="WithChildren">
                <label class="form-check-label" for="inlineRadio1">With children</label>
            </div>    
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="WithChildren">
                <label class="form-check-label" for="inlineRadio2">No children</label>
            </div>    
        </td>
        <td>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="WithChildren">
                <label class="form-check-label" for="inlineRadio1">No opinion</label>
            </div>
        </td>
    </tr>
    </table>
    <br />

    <a href="#" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">Submit</a>
    <a href="#" class="btn btn-secondary btn-lg active" role="button" aria-pressed="true" onclick="clear()">Clear</a>

</div>

@section scripts {
    <script>
        function clear() {
            
        }

        function submit() {
            
        }
    </script>
}